+***********************************************************************/
+rzv2m_drp* rzv2m_drp::get_instance_forapi()
+{
+    drp_init_for_api_mutex.lock();
+
+    /* check initialize if not exec initialize */
+    if(_singleton == NULL) 
+    {
+        _singleton = new rzv2m_drp();
+        _singleton->initialize();
